{
  "name": "@kitsonk/kview-panel",
  "version": "0.0.1",
  "exports": { "./install": "./install.ts" },
  "nodeModulesDir": "auto",
  "tasks": {
    "check": "deno fmt --check . && deno lint . && deno check",
    "dev": "vite",
    "build": "vite build",
    "start": "deno serve -A _fresh/server.js",
    "update": "deno run -A -r jsr:@fresh/update ."
  },
  "unstable": ["kv"],
  "lint": { "rules": { "tags": ["fresh", "recommended"] } },
  "fmt": { "lineWidth": 120 },
  "exclude": [
    "**/_fresh/*"
  ],
  "imports": {
    "@deno/kv-utils": "jsr:@deno/kv-utils@^0.1.4",
    "@fresh/plugin-vite": "jsr:@fresh/plugin-vite@^1.0.5",
    "@kitsonk/kv-toolbox": "jsr:@kitsonk/kv-toolbox@^0.30.0",
    "@logtape/logtape": "jsr:@logtape/logtape@^1.1.1",
    "@oak/commons": "jsr:@oak/commons@^1.0.1",
    "@preact/signals": "npm:@preact/signals@^2.3.1",
    "@std/assert": "jsr:@std/assert@^1.0.15",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.5",
    "@std/encoding": "jsr:@std/encoding@^1.0.10",
    "@std/fmt": "jsr:@std/fmt@^1.0.8",
    "@std/html": "jsr:@std/html@^1.0.5",
    "@std/path": "jsr:@std/path@^1.1.2",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.12",
    "fresh": "jsr:@fresh/core@^2.1.2",
    "object-inspect": "npm:object-inspect@^1.13.4",
    "preact": "npm:preact@^10.27.2",
    "tailwindcss": "npm:tailwindcss@^4.1.10",
    "vite": "npm:vite@^7.1.3"
  },
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.asynciterable",
      "dom.iterable",
      "deno.ns"
    ],
    "jsx": "precompile",
    "jsxImportSource": "preact",
    "jsxPrecompileSkipElements": [
      "a",
      "img",
      "source",
      "body",
      "html",
      "head",
      "title",
      "meta",
      "script",
      "link",
      "style",
      "base",
      "noscript",
      "template"
    ],
    "types": [
      "vite/client"
    ]
  }
}
